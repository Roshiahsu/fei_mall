<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.mall.mapper.ProductTypeMapper">

<!--    List<ProductTypeListVO> listProductType();-->
    <select id="listProductType" resultType="cn.tedu.mall.pojo.product.ProductTypeListVO">
        select
            id,
            name
        from
            pms_product_type
        Order by
            id
    </select>
</mapper>
